

<% @near_events.each do |event| %>
  </div>
    <h2><%=event.title%></h2>
    <p><%=event.address%></p>
    <p><%=event.date%></p>
  </div>
<% end %>


<div class="map-container">
  <div
    id="map"
    style="width: 100%; height: 100vh; top: 0; margin-left: 15px; margin-top: 5px;"
    data-markers="<%= @markers.to_json %>">
  </div>
</div>


